import { tally } from "./stv"

test('tallies vote using stv without a quota', () => {

    const ballots = [
	{ id: 1, votes: { a: 1, b: 2, c: 3 }},
	{ id: 2, votes: { a: 1, b: 2, c: 3 }},
	{ id: 3, votes: { a: 1, b: 2, c: 3 }},
	{ id: 4, votes: { a: 1, b: 2, c: 3 }},
	{ id: 5, votes: { a: 1, b: 2, c: 3 }},
    ]
    
    expect(tally([])).toStrictEqual([])
})
